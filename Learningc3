#include<stdio.h>
#include<conio.h>
void main()
{
	int a,sum=0,i=0;
	while(i<=5)
	{
		printf("\n enter the number");
		scanf("%d",&a);
		i++;
		if(a<0)
		{
			printf("\n entered number is odd and program terminating");
			continue;
		}
		sum=sum+a;
		printf("\n entered number is positive ");
	}
	sum=sum+a;
	printf("\n sum is %d ",sum);
}
